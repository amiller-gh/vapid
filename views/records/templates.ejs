<h1 class="heading">
  <%- include('../toggle') %>
  <%- title %>
</h1>

<section class="section page-templates">
  <h2 class="page-templates__header">Select a Page Template</h2>
  <ul class="page-templates__list">
    <% for (template of templates) { if (!template.hasView) { continue; } %>
      <li class="page-templates__template">
        <a href="/dashboard/new/page/<%- template.name %>"><%- template.name === 'index' ? 'Home' : template.name %></a>
      </li>
    <% } %>
  </ul>
</section>
